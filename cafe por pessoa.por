programa {
  funcao inicio() {
    real cafe, cafe_per_capita
    inteiro alunos
    escreva("quantos litos de cafe ")
    leia(cafe)
    escreva("quantos alunos tem hoje? ")
    leia (alunos)
    cafe_per_capita= cafe/alunos
